{
    "name": "@arrival/doc-builder",
    "version": "0.0.11",
    "private": false,
    "scripts": {
        "build:package": "webpack --display-error-details --config webpack.config.package.js --p --progress --mode production && yarn bundle-dts",
        "bundle-dts": "node node_modules/@eigenspace/helper-scripts/scripts/project/bundle-dts.js",
        "lint": "tslint --project tsconfig.json",
        "test": "jest --coverage",
        "test:ci": "jest --ci --coverage",
        "tsc": "tsc",
        "deploy:publish": "node node_modules/@eigenspace/helper-scripts/scripts/ci/publish.js",
        "hooks:pre-push": "yarn lint && yarn test",
        "script:update-deps": "node node_modules/@eigenspace/helper-scripts/scripts/project/update-deps.js"
    },
    "husky": {
        "hooks": {
            "pre-push": "yarn hooks:pre-push"
        }
    },
    "lint-staged": {
        "*.{ts,tsx}": [
            "yarn lint"
        ]
    },
    "dependencies": {
        "@phenomnomnominal/tsquery": "3.0.0"
    },
    "devDependencies": {
        "@eigenspace/codestyle": "latest",
        "@eigenspace/helper-scripts": "1.0.48",
        "@types/jest": "22.2.2",
        "@types/node": "11.9.5",
        "clean-webpack-plugin": "1.0.1",
        "copy-webpack-plugin": "4.6.0",
        "husky": "1.3.1",
        "jest": "23.6.0",
        "ts-jest": "23.10.5",
        "ts-loader": "5.3.3",
        "tslint": "5.12.1",
        "typescript": "3.3.1",
        "webpack": "4.29.0",
        "webpack-cli": "3.2.1"
    },
    "main": "index.js",
    "types": "index.d.ts",
    "repository": {
        "type": "git",
        "url": "https://github.com/cybernated/web-doc-builder.git"
    },
    "license": "MIT"
}
